{
  "name": "api",
  "version": "1.0.0",
  "description": "api",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/test\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/test/**/*.js && npm run posttest",
    "prestart": "npm run build",
    "start": "node .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "dist/index*",
    "src"
  ],
  "dependencies": {
    "@loopback/authentication": "^1.2.1",
    "@loopback/boot": "^1.2.8",
    "@loopback/context": "^1.16.0",
    "@loopback/core": "^1.7.1",
    "@loopback/dist-util": "^0.4.0",
    "@loopback/docs": "^1.19.0",
    "@loopback/example-hello-world": "^1.1.18",
    "@loopback/example-log-extension": "^1.1.18",
    "@loopback/example-rpc-server": "^1.1.17",
    "@loopback/example-soap-calculator": "^1.5.1",
    "@loopback/example-todo": "^1.5.13",
    "@loopback/example-todo-list": "^1.6.1",
    "@loopback/http-caching-proxy": "^1.0.20",
    "@loopback/http-server": "^1.2.4",
    "@loopback/metadata": "^1.1.5",
    "@loopback/openapi-spec-builder": "^1.1.11",
    "@loopback/openapi-v3": "^1.4.0",
    "@loopback/openapi-v3-types": "^1.0.19",
    "@loopback/repository": "^1.5.6",
    "@loopback/repository-json-schema": "^1.4.8",
    "@loopback/rest": "^1.12.0",
    "@loopback/rest-explorer": "^1.1.23",
    "@loopback/service-proxy": "^1.1.11",
    "memory": "0.0.3",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@loopback/build": "^1.5.5",
    "@loopback/testlab": "^1.2.10",
    "@types/node": "^10.11.2",
    "@types/passport": "^1.0.0",
    "@types/passport-http": "^0.3.8"
  }
}
